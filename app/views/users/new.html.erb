

<%= form_tag("/users", method: "post") do %>
    <%= label_tag(:user, "Create New User:") %>
    <%= text_field_tag(:user) %>
    <%= label_tag(:email, "Email:") %>s
    <%= text_field_tag(:email) %>
    <%= label_tag(:password, "Password:") %>
    <%= text_field_tag(:password) %>
    <%= submit_tag("Submit") %>
  <% end %>

